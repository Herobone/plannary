rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /userData/{userID}/exams/{examID} {
      allow read, write: if request.auth.uid != null &&
                                 userID == request.auth.uid;
    }

    match /{document=**} {
        allow read, write: if false;
    }
  }
}
